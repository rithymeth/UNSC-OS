{
    "providers": {
        "aws": {
            "enabled": true,
            "region": "us-west-2",
            "bucket": "unsc-os-storage"
        },
        "azure": {
            "enabled": false,
            "connection_string": "",
            "container": "unsc-os-storage"
        },
        "gcp": {
            "enabled": false,
            "project": "",
            "bucket": "unsc-os-storage"
        }
    },
    "sync": {
        "interval": 300,
        "max_file_size": 104857600,
        "excluded_extensions": [
            ".tmp",
            ".log",
            ".lock",
            ".part"
        ],
        "sync_folders": [
            "documents",
            "pictures",
            "downloads"
        ]
    }
}